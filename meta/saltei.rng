                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(italics) </a:documentation>
                        <value>#gap</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(inline gap) </a:documentation>
                        <value>#rt</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(recte) </a:documentation>
                        <value>#sc</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(small caps) </a:documentation>
                        <value>#spc</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(spaced out) </a:documentation>
                        <value>#sup</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(superscript) </a:documentation>
                        <value>#sub</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(subscript) </a:documentation>
