               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A section specially labeled as 'Partida' (not to be confused with @type='part').</a:documentation>
               <value>law</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A section labeled as 'Lex', or as something equivalent.</a:documentation>
               <value>preface</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">An introductory statement about the work/volume, written by its author.</a:documentation>
               <value>privileges</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>question</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A section labeled as 'quaestio' (or as something equivalent).</a:documentation>
               <value>section</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A somewhat arbitrary label for sections of any size, usually occurring in the main body of a work/volume. NOTE: for sections labeled as 'Sectio' (or something equivalent) please refer to @type='segment').</a:documentation>
               <value>segment</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A section labeled as 'Sectio', or as something equivalent.</a:documentation>
               <value>source</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A section from a different work (possibly by a different author), embedded in the current text.</a:documentation>
               <value>title</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A section labeled as 'Titulus'/'Titulo', or as something equivalent.</a:documentation>
               <value>unknown</value>
